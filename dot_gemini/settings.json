{
    "general": {
        "checkpointing": {
            "enabled": true
        }
    },
    "ide": {
        "enabled": true,
        "hasSeenNudge": true
    },
    "mcpServers": {
        "context7": {
            "headers": {
                // "CONTEXT7_API_KEY": "YOUR_API_KEY",
                "Accept": "application/json, text/event-stream"
            },
            "httpUrl": "https://mcp.context7.com/mcp"
        },
        "fetch": {
            "args": [
                "mcp-server-fetch"
            ],
            "command": "uvx"
        },
        "git": {
            "args": [
                "mcp-server-git"
            ],
            "command": "uvx"
        },
        "memory": {
            "args": [
                "-y",
                "@modelcontextprotocol/server-memory"
            ],
            "command": "npx"
        },
        "sequential-thinking": {
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ],
            "command": "npx"
        },
        "time": {
            "args": [
                "mcp-server-time"
            ],
            "command": "uvx"
        }
    },
    "output": {
        "format": "text"
    },
    "security": {
        "auth": {
            "selectedType": "oauth-personal"
        }
    },
    "tools": {
        "shell": {
            "showColor": true
        }
    },
    "ui": {
        "showCitations": true,
        "showLineNumbers": true,
        "showMemoryUsage": true,
        "showStatusInTitle": true
    }
}