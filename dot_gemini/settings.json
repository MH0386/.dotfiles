{
  "context": { "fileName": ["AGENTS.md", "CONTEXT.md", "GEMINI.md"] },
  "general": {
    "checkpointing": { "enabled": true },
    "enablePromptCompletion": true
  },
  "ide": { "enabled": true, "hasSeenNudge": true },
  "mcpServers": {
    "agno-docs": { "httpUrl": "https://docs.agno.com/mcp" },
    "gradio-docs": {
      "url": "https://gradio-docs-mcp.hf.space/gradio_api/mcp/sse"
    },
    "byterover-mcp": { "httpUrl": "https://mcp.byterover.dev/v2/mcp" },
    "fetch": { "args": ["mcp-server-fetch"], "command": "uvx" },
    "git": { "args": ["mcp-server-git"], "command": "uvx" },
    "memory": {
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "command": "npx"
    },
    "sequential-thinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx"
    },
    "time": { "args": ["mcp-server-time"], "command": "uvx" }
  },
  "output": { "format": "text" },
  "security": { "auth": { "selectedType": "oauth-personal" } },
  "tools": { "shell": { "showColor": true } },
  "ui": {
    "showCitations": true,
    "showLineNumbers": true,
    "showMemoryUsage": true,
    "showStatusInTitle": true,
    "useFullWidth": true
  }
}
