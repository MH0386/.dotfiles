{
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "chat.agent.todoList": {
    "position": "default"
  },
  "chat.agentSessionsViewLocation": "view",
  "chat.checkpoints.showFileChanges": true,
  "chat.edits2.enabled": true,
  "chat.emptyState.history.enabled": true,
  "chat.mcp.gallery.enabled": true,
  "chat.tools.terminal.autoApprove": {
    "nix-prefetch-url": true
  },
  "chat.tools.urls.autoApprove": {
    "https://*.github.com": true
  },
  "chat.useCloudButtonV2": true,
  "chat.useNestedAgentsMdFiles": true,
  "diffEditor.codeLens": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "docker.extension.enableComposeLanguageServer": false,
  "doppler.autocomplete.enable": true,
  "doppler.hover.enable": true,
  "editor.aiStats.enabled": true,
  "editor.autoIndentOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.convertImportFormat": "always",
    "source.fixAll": "always",
    "source.fixAll.sortJSON": "always",
    "source.organizeImports.ruff": "always",
    "source.sort.json": "always",
    "source.unusedImports": "always"
  },
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono, ZedMono Nerd Font Mono, monospace",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "errorLens.codeLensEnabled": true,
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.sh.tmpl": "shellscript",
    "*.yaml.tmpl": "yaml",
    "*gitconfig": "properties",
    "*ignore": "ignore",
    "*zshrc": "shellscript",
    ".xonshrc": "xonsh",
    "compose-*.yaml": "dockercompose",
    "dot_xonshrc": "xonsh"
  },
  "files.autoSave": "afterDelay",
  "gemini-cli.debug.logging.enabled": true,
  "geminicodeassist.inlineSuggestions.nextEditPredictions": true,
  "geminicodeassist.updateChannel": "Default",
  "git.allowNoVerifyCommit": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autofetch": true,
  "git.confirmNoVerifyCommit": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "always",
  "git.replaceTagsWhenPull": true,
  "github-actions.workflows.pinned.refresh.enabled": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "githubLocalActions.dockerDesktopPath": "C:/Program Files/Docker/Docker/Docker Desktop.exe",
  "githubPullRequests.pullBranch": "never",
  "gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
  "gitlens.ai.model": "gemini:gemini-2.5-flash",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "inlineChat.enableV2": true,
  "inlineChat.notebookAgent": true,
  "json.schemaDownload.trustedDomains": {
    "https://json-schema.org/": true,
    "https://json.schemastore.org/": true,
    "https://opencode.ai": true,
    "https://raw.githubusercontent.com/": true,
    "https://schemastore.azurewebsites.net/": true,
    "https://www.schemastore.org/": true
  },
  "json.schemas": [],
  "mermaid-chat.enabled": true,
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "path-intellisense.showHiddenFiles": true,
  "python.analysis.aiHoverSummaries": true,
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.enableTroubleshootMissingImports": true,
  "python.analysis.generateWithTypeAnnotation": true,
  "python.analysis.includeAliasesFromUserFiles": true,
  "python.analysis.inlayHints.callArgumentNames": "all",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.supportDocstringTemplate": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.languageServer": "None",
  "python.testing.pytestArgs": [
    "tests"
  ],
  "python.testing.pytestEnabled": true,
  "redhat.telemetry.enabled": true,
  "remote.autoForwardPortsSource": "hybrid",
  "ruff.format.preview": true,
  "security.allowedUNCHosts": [
    "wsl.localhost"
  ],
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "Nushell",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.ignoreBracketedPasteMode": true,
  "terminal.integrated.mouseWheelZoom": true,
  "terminal.integrated.profiles.linux": {
    "bash": {
      "icon": "terminal-bash",
      "path": "bash"
    },
    "fish": {
      "path": "fish"
    },
    "nu": {
      "path": "nu"
    },
    "pwsh": {
      "icon": "terminal-powershell",
      "path": "pwsh"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    },
    "zsh": {
      "path": "zsh"
    }
  },
  "terminal.integrated.profiles.windows": {
    "Nushell": {
      "icon": "terminal",
      "path": "nu"
    }
  },
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.suggest.providers": {
    "lsp": true,
    "terminal-suggest": true
  },
  "terminal.integrated.suggest.quickSuggestions": {
    "arguments": "on",
    "commands": "on",
    "unknown": "off"
  },
  "update.mode": "none",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.experimental.share.enabled": true,
  "yaml.format.singleQuote": true
}
