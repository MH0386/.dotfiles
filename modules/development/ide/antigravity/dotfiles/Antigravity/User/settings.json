{
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "chat.mcp.gallery.enabled": true,
  "dev.containers.experimentalLockfile": true,
  "diffEditor.codeLens": true,
  "doppler.autocomplete.enable": true,
  "doppler.hover.enable": true,
  "editor.aiStats.enabled": true,
  "editor.defaultFormatter": "trunk.io",
  "editor.fontFamily": "'Cascadia Code', 'Droid Sans Mono', 'monospace', monospace",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "errorLens.codeLensEnabled": true,
  "files.associations": {
    "*.sh.tmpl": "shellscript",
    "*.yaml.tmpl": "yaml",
    "*gitconfig": "properties",
    "*ignore": "ignore",
    "*zshrc": "shellscript",
    "compose-*.yaml": "dockercompose"
  },
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "gemini-cli.debug.logging.enabled": true,
  "geminicodeassist.inlineSuggestions.nextEditPredictions": true,
  "geminicodeassist.updateChannel": "Default",
  "git.allowNoVerifyCommit": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autofetch": true,
  "git.confirmNoVerifyCommit": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.openRepositoryInParentFolders": "always",
  "github-actions.workflows.pinned.refresh.enabled": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.gitProtocol": "ssh",
  "gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
  "gitlens.ai.model": "gemini:gemini-2.5-flash",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "mdb.mcp.server": "autoStartEnabled",
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "path-intellisense.showHiddenFiles": true,
  "python.analysis.aiHoverSummaries": true,
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.cacheLSPData": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.enableTroubleshootMissingImports": true,
  "python.analysis.generateWithTypeAnnotation": true,
  "python.analysis.includeAliasesFromUserFiles": true,
  "python.analysis.inlayHints.callArgumentNames": "all",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.supportDocstringTemplate": true,
  "remote.autoForwardPortsSource": "hybrid",
  "ruff.format.preview": true,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.mouseWheelZoom": true,
  "update.mode": "none",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.experimental.share.enabled": true
}
